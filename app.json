{
  "expo": {
    "name": "Feedget_mobile",
    "slug": "feedget_mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "Widget created to send feedback on problems, ideas, or other topics.\nYou put the message in it and take a screenshot of the cell phone screen showing the subject in question.",
    "githubUrl": "https://github.com/lucas-rabelo/Feedget_mobile"
  }
}
